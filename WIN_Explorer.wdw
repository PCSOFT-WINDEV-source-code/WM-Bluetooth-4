     name : WriteValue
     procedure_id : 1762596252639021871
     type_code : 14
     code : |1+
      // Summary: Writes the value of a characteristic
      // Syntax:
      //WriteValue (<sServiceUUID> is string, <sCharacteristicUUID> is string)
      //
      // Parameters:
      //	sServiceUUID (UNICODE string): UUID of the service
      //	sCharacteristicUUID (UNICODE string): UUID of the characteristic
      
      PROCEDURE WriteValue(sServiceUUID is string, sCharacteristicUUID is string)
      
      FOR EACH service of gBTLEDevice.Service
      	IF service.UUID = sServiceUUID THEN
      		FOR EACH characteristic of service.caracteristique
      			IF characteristic.UUID = sCharacteristicUUID THEN
      				//1: OK
      				//2: Cancel
      				sEdit is string ANSI = BTLECharacteristicReadValue(characteristic)
      				SWITCH Input(<§?173ED68D00F58C0A§>, sEdit)
      					// OK
      					CASE 1
      						bufValue is buffer =  sEdit
      						IF BTLECharacteristicWriteValue(characteristic,bufValue) THEN
      							ToastDisplay("Value successfully modified")
      						ELSE
      							Error("Error while modifying the value: " + ErrorInfo)
      						END
      						
      					// Cancel
      					CASE 2
      						
      				END
      			END
      		END
      	END
      END
