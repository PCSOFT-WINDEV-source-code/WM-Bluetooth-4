     name : InsubscribeValue
     procedure_id : 1762596252638956335
     type_code : 14
     code : |1-
      // Summary: Unsubscribe from a value
      //
      PROCEDURE InsubscribeValue()
      
      IF gSubscriberCharacteristic..UUID <> "" THEN
      	// Unsubscribe
      	BTLECharacteristicChangeOfValue(gSubscriberCharacteristic)
      	gSubscriberCharacteristic = new BTLECharacteristic
      	GR_Subscription..Visible = False
      ELSE
      	Error("Unable to unsubscribe from the value.")
      END
