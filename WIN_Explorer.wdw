     internal_properties : CAAAAAgAAADMHnZiD+aDnBow/jSKmGeGuQ0uSh+8Sw1MdslBQbPl7y+BIA3b6NYwGyAROltri2LkHuhUd0y2Xcv59j20B+U/8Ei0KPtPn49o8Owd52khlqpLuOp5x+PGnMyd9dQz31IM5xY4WhNKG1T+DQrA4CwepKd9TjEUisawOKetCM4XonLEtzFIpC7y/+CLV6aKcTzGUXrWebKDNcYTpwhLeJFcvoEmYawFKUhs9Com8avlFoFxZL+CQJBs4xJbE7c=
     procedure_id : 1762596252638628655
     type_code : 14
     code : |1-
      // Summary: Callback called after the connection to a BTLE device
      //
      PROCEDURE CB_Connect(mydevice is a BTLEDevice <useful>, bResult is boolean)
      
      IF bResult THEN
      	// Connection established
      	AnalyzeDevice()
      ELSE
      	Error("Unable to connect to this Bluetooth sensor")
      	Close()
      END
     type : 458752
   -
     name : AnalyzeDevice
     internal_properties : CAAAAAgAAACWt8fzT/Se8a5Cj/jvyavuqNHWlSpXWfw1G5oU8EGwt/ajMfEX23EkqfYr+bgwdmnsW4Wg2sRdXMu7KJIbmiWYPfu/aEwiy/6bVPoFF8qzOD+c99oxhFNIagRREm/iA9JXNE7VtjuokcaAsilN
     procedure_id : 1762596252638694191
     type_code : 14
     code : |1-
      // Summary: Analyzes the device
      //
      PROCEDURE AnalyzeDevice()
      
      MyWindow..Plane = PLANE_EXPLORATION
      FOR EACH service of gBTLEDevice.Service
      	// Service
      	sServiceName is string = BTLEServiceName(service.UUID)
      	
      	IF sServiceName ~= "" THEN
      		sServiceName = "Unknown service" 
      	END
      	
      	FOR EACH characteristic of service.caracteristique
      		sProperties is string
      		arrProperties is array of int
      		
      		// Retrieve the characteristic properties
      		arrProperties = BTLEPropertyCharacteristic(characteristic)
      		
      		// Naming properties
      		IF ArraySeek(arrProperties,asLinear,PROPERTY_BROADCAST) > 0 THEN sProperties +=  [","] + "BROADCAST"
      		IF ArraySeek(arrProperties,asLinear,PROPERTY_EXTENDED_PROPS) > 0 THEN sProperties +=  [","] + "EXTENDED_PROPS"
      		IF ArraySeek(arrProperties,asLinear,PROPERTY_INDICATE) > 0 THEN sProperties +=  [","] + "INDICATE"
      		IF ArraySeek(arrProperties,asLinear,PROPERTY_NOTIFY) > 0 THEN sProperties +=  [","] + "NOTIFICATION"
      		IF ArraySeek(arrProperties,asLinear,PROPERTY_READ) > 0 THEN sProperties +=  [","] + "READ"
      		IF ArraySeek(arrProperties,asLinear,PROPERTY_SIGNED_WRITE) > 0 THEN sProperties +=  [","] + "SIGNED WRITE"
      		IF ArraySeek(arrProperties,asLinear,PROPERTY_WRITE) > 0 THEN sProperties +=  [","] + "WRITING"
      		IF ArraySeek(arrProperties,asLinear,PROPERTY_WRITE_NO_RESPONSE) > 0 THEN sProperties +=  [","] + "WRITING WITHOUT RESPONSE"
      		
      		// Add into the looper
      		LooperAddLine(LOOP_Info,service.UUID,sServiceName,characteristic.UUID , "Properties: "  + sProperties, characteristic..Property)
      	END
      END
      
      
      LooperCollapseAll(LOOP_Info)
     type : 458752
   -
     name : ReadValue
     procedure_id : 1762596252638759727
     type_code : 14
     code : |1-
      // Summary: Reads the value of a characteristic
      // Syntax:
      //ReadValue (<sServiceUUID> is string, <sCharacteristicUUID> is string)
      //
      // Parameters:
      //	sServiceUUID (UNICODE string): UUID of the service
      //	sCharacteristicUUID (UNICODE string): UUID of the characteristic
      
      PROCEDURE ReadValue(sServiceUUID is string, sCharacteristicUUID is string)
      
      FOR EACH service of gBTLEDevice.Service
      	IF service.UUID = sServiceUUID THEN
      		FOR EACH characteristic of service.caracteristique
      			IF characteristic.UUID = sCharacteristicUUID THEN
      				// Characteristic found
      				bufFerValue is buffer = BTLECharacteristicReadValue(characteristic)
      				
      				// Value
      				sBuf is string ANSI = bufFerValue
      				Info("Value of the characteristic: " + sBuf)
      				RETURN
      			END
      		END
      	END
      END
      
      Error("Unable to read the value.")
     type : 458752
   -
     name : SubscribeValue
     procedure_id : 1762596252638825263
     type_code : 14
     code : |1+
      // Summary: Subscribe to the value of a characteristic
      // Syntax:
      //SubscribeValue (<sServiceUUID> is string, <sCharacteristicUUID> is string)
      //
      // Parameters:
      //	sServiceUUID (UNICODE string): UUID of the service
      //	sCharacteristicUUID (UNICODE string): UUID of the characteristic
      
      PROCEDURE SubscribeValue(sServiceUUID is string, sCharacteristicUUID is string)
      
      
      FOR EACH service of gBTLEDevice.Service
      	IF service.UUID = sServiceUUID THEN
      		FOR EACH characteristic of service.caracteristique
      			IF characteristic.UUID = sCharacteristicUUID THEN
      				// Characteristic found
      				// Subscribe
      				gSubscriberCharacteristic = characteristic
      				STC_SubscriptionValue = ""
      				BTLECharacteristicChangeOfValue(characteristic,CB_ChangeValue)
      				GR_Subscription..Visible = True
      				RETURN
      			END
      		END
      	END
      END
      Error("Unable to subscribe to the value.")
      
