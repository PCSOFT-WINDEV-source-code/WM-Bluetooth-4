     name : Connected
     procedure_id : 1762596252638563119
     type_code : 14
     code : |1-
      // Summary: Connects the device
      //
      PROCEDURE Connected()
      
      MyWindow..Plane = PLAN_CONNECTION
      
      // Check whether the connection was not already established
      IF BTLEConnectionStatus(gBTLEDevice) = btleStatusConnected THEN
      	CB_Connect(gBTLEDevice,True)
      ELSE
      	// Start the connection
      	BTLEConnect(gBTLEDevice,CB_Connect)
      END
